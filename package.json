{
  "name": "block-builder",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/rjschie/block-builder.git"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "setup": "yarn node scripts/setup.js"
  },
  "importSort": {
    ".js, .jsx, .es, .cjs": {
      "parser": "babylon",
      "style": "eslint"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "eslint"
    }
  },
  "engines": {
    "node": "14.* || >= 16"
  },
  "dependencies": {
    "eslint": "^8.9.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "husky": "^7.0.4",
    "import-sort-style-eslint": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "prettier-plugin-import-sort": "^0.0.7",
    "pretty-quick": "^3.1.3"
  },
  "packageManager": "yarn@3.1.0"
}
